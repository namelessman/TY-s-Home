<!--pages/man/man.wxml-->
<view class="container">
      <view class='gas' wx:if="{{isGas}}">警告:可燃气体状态异常！</view>
        <view class='circle'>
        <view>温度{{temperature}}℃</view>
        <view>湿度{{humidity}}%</view>
      </view>

      <button class='seven' bindtap='goSeven'>查看一周趋势</button>

      <image mode='aspectFill' src='../../static/img/env.jpg' class='bg'></image>
</view>
